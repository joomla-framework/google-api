{
    "name": "joomla/google",
    "type": "joomla-package",
    "description": "Joomla Google Package",
    "keywords": ["joomla", "framework", "google"],
    "homepage": "https://github.com/joomla-framework/google-api",
    "license": "GPL-2.0+",
    "require": {
        "php": "^5.5.9|~7.0",
        "joomla/uri": "~1.0|~2.0",
        "joomla/oauth2": "~2.0@dev"
    },
    "require-dev": {
        "joomla/input": "~1.0|~2.0",
        "joomla/registry": "^1.4.5|~2.0",
        "joomla/session": "~1.0|~2.0",
        "joomla/test": "~1.0",
        "phpunit/phpunit": "^4.8.35|^5.4.3|~6.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Google\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Google\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
