{
    "name": "joomla/google",
    "type": "joomla-package",
    "description": "Joomla Google Package",
    "keywords": ["joomla", "framework", "google"],
    "homepage": "https://github.com/joomla-framework/google-api",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/registry": "~1.0",
        "joomla/uri": "~1.0",
        "joomla/oauth2": "~1.0",
        "joomla/filesystem": "~1.0"
    },
    "require-dev": {
    	"joomla/input": "~1.0",
    	"joomla/test": "~1.0"
    },
    "autoload": {
    	"psr-4": {
    		"Joomla\\Google\\": "src/"
    	}
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Google\\Tests\\": "Tests/"
        }
    }
}
